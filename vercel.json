{
  "version": 2,
  "builds": [
    {
      "src": "Rigmedia/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize":  "15mb", "runtime":  "python3.10" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "RigMedia/wsgi.py"
    }
  ]
}
